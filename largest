const num1 = 20;
const num2 = 35;
const num3 = 28;
let largest;
// check the condition
if(num1 >= num2 && num1 >= num3){
    largest = num1;
}else if(num2 >= num1 && num2 >= num3){
    largest = num2;
}else{
   largest = num3;
}
// display the result
console.log('the largest number is ' + largest);
